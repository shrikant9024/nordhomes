{
    "builds": [
        {
            "src": "your_project_name/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.9"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "your_project_name/wsgi.py"
        }
    ]
}